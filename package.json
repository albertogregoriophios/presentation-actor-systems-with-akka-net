{
    "name": "presentation-actor-systems-with-akka-net",
    "version": "1.0.0",
    "description": "Presentation Actor Model and Akka.net",
    "main": "index.js",
    "scripts": {
        "test": "reveal-md presentation/english.md --css presentation.css --preprocessor emoji2svgimg.js",
        "serve": "reveal-md presentation/english.md --css presentation.css --preprocessor emoji2svgimg.js -w",
        "serve-de": "reveal-md presentation/deutsch.md --css presentation.css --preprocessor emoji2svgimg.js -w",
        "build": "reveal-md presentation/english.md --css presentation.css --static docs --preprocessor emoji2svgimg.js --assets-dir=assets",
        "build-de": "reveal-md presentation/deutsch.md --css presentation.css --static docs_de --preprocessor emoji2svgimg.js --assets-dir=assets",
        "export-pdf": "reveal-md presentation/english.md --css presentation.css --print english.pdf --preprocessor emoji2svgimg.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/marsop/presentation-actor-systems-with-akka-net.git"
    },
    "keywords": [
        "revealjs",
        "akkanet",
        "dotnet",
        "actor_model"
    ],
    "author": "Alberto Gregorio",
    "license": "CC-BY-SA-4.0",
    "bugs": {
        "url": "https://github.com/marsop/presentation-actor-systems-with-akka-net/issues"
    },
    "homepage": "https://github.com/marsop/presentation-actor-systems-with-akka-net#readme",
    "dependencies": {
        "reveal-md": "^3.4.7",
        "twemoji": "^12.1.5"
    }
}